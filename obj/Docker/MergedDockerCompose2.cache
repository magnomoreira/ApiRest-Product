services:
  product.apirest:
    build:
      context: C:\Users\Administrador\Documents\dev\ApiRest-Product
      dockerfile: Product.ApiRest/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Product.ApiRest
      target: base
    container_name: Product.ApiRest
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: productapirest:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/Product.ApiRest.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\Administrador\Documents\dev\ApiRest-Product\Product.ApiRest:/app:rw
    - C:\Users\Administrador\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Administrador\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Administrador\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Administrador\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Administrador\Documents\dev\ApiRest-Product:/src:rw
version: '3.4'